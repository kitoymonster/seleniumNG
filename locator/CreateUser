	// user management - registration
	public static WebElement registrationTab(WebDriver driver) throws InterruptedException {
		TraceLog.start("UM_Register.registrationTab");
		
		element = GetElementBy.byXpath(driver, "/html/body/div[1]/div[1]/div[1]/div[4]/ul/li[1]/a");

		TraceLog.ended("UM_Register.registrationTab");
		return element;
	}
	
